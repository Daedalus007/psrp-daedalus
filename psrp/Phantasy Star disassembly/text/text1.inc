.stringmap script "ハプスビーガ<line>"
.stringmap script "コワレタトコロヲ　ナオシタノデ<wait more>"
textShionFoundFlowMover_a:
.stringmap script "ツカエルヨウニ　ナッタゾ。<wait>"
